import java.util.*;

public class Main {

    public static void main(String[] args) {
        Scanner scn = new Scanner(System.in);

        // write ur code here
        int x=scn.nextInt();
        int i;
        
        for(i=1;i<=10;i++){
            int value = x*i;
            System.out.println(x +" * " + i +" = " + value);        
        }

    }
}
